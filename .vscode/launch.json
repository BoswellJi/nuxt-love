{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "nuxt Debug",
      "request": "launch",
      "runtimeArgs": ["nuxt", "dev"],
      "runtimeExecutable": "yarn",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "NODE_ENV": "development"
      },
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std",
      "type": "node"
    }
  ]
}
